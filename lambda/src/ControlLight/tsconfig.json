{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 18",

  "_version": "2.0.0",

  "compilerOptions": {
    "module": "commonjs",
    "target": "ESNext",

    "noEmit": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node"
  },
  "exclude": [
    "node_modules"
  ]
}
